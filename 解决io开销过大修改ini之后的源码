		for (int i = 0; i < powerHeadRoomLen; i++) {
			int num = Integer.parseInt(valueItems[i]);
			powerHeadRooms[i] += num;
			powerHeadRoomSum += parse.getPowerHeadRoomValue(i) * num;
			powerHeadRoomCnt += num;
		}
