public class LteNcInfo {
	private int _rsrp = -1;
	private String _earfcnPci = "";
	
	public void setValue(int rsrp, String earfcnPci) {
		_rsrp = rsrp;
		_earfcnPci = earfcnPci;
	}
	
	public int getRsrp() {
		return _rsrp;
	}
	
	public String getEarfcnPci() {
		return _earfcnPci;
	}
